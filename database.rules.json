{
  "rules": {
    "monsters": {
      ".read": true,
      ".write": false
    },
    "custom_monsters": {
      ".read": true,
      ".write": "$uid === auth.uid"
    },
    "spells": {
      ".read": true,
      ".write": false
    },
    "userdata": {
      "$uid": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
      }
    },
    ".read": false,
    ".write": false
  }
}
